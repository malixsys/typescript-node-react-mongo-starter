{
  "name": "typescript-node-react-parcel-starter",
  "version": "1.0.0",
  "description": "Starter App Using Node, TypeScript, React and Parcel",
  "private": true,
  "main": "dist/server/index.js",
  "repository": {
    "url": "https://github.com/malixsys/typescript-node-react-parcel-starter.git"
  },
  "keywords": [
    "typescript",
    "okta",
    "node",
    "react",
    "parcel",
    "parcel-bundler"
  ],
  "author": "malixsys",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.2",
    "@types/mongoose": "^5.7.0",
    "@types/node": "13.7.0",
    "@types/parcel-bundler": "1.12.1",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/socket.io": "2.1.4",
    "@types/uuid": "3.4.7",
    "parcel-bundler": "1.12.4",
    "ts-node-dev": "1.0.0-pre.44",
    "tslint": "^6.0.0",
    "typescript": "3.7.5"
  },
  "dependencies": {
    "@okta/jwt-verifier": "1.0.0",
    "@okta/okta-react": "1.4.0",
    "@okta/okta-sdk-nodejs": "3.1.0",
    "axios": "^0.19.2",
    "colors": "^1.4.0",
    "dotenv": "8.2.0",
    "express": "4.19.2",
    "gridfs-stream": "^1.1.1",
    "immutable": "4.0.0-rc.12",
    "mongoose": "^5.8.11",
    "mongoose-timestamp": "^0.6.0",
    "prettier": "^1.19.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.1",
    "socket.io": "2.3.0",
    "socket.io-client": "2.3.0",
    "typedi": "^0.8.0",
    "uuid": "3.4.0"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true
  },
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build:server": "NODE_ENV=production tsc",
    "build:client": "NODE_ENV=production parcel build ./src/client/index.html -d dist/client",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "postinstall": "npm run build",
    "start": "NODE_ENV=production node dist/server/index.js",
    "dev": "ts-node-dev -r dotenv/config --debug --watch=.src/server --no-deps --respawn --transpileOnly -- ./src/server/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
